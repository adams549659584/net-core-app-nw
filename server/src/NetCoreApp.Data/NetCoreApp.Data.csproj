<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AssemblyName>Beginor.NetCoreApp.Data</AssemblyName>
    <RootNamespace>Beginor.NetCoreApp.Data</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.60.6" />
    <PackageReference Include="NHibernate" Version="5.2.6" />
    <PackageReference Include="NHibernate.AspNetCore.Identity" Version="3.0.0" />
    <PackageReference Include="Beginor.AppFx.Repository.Hibernate" Version="3.0.0" />
    <PackageReference Include="Beginor.AppFx.Core" Version="3.0.0" />
    <PackageReference Include="NHibernate.Extensions.NpgSql" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Entities/*.hbm.xml" />
    <EmbeddedResource Include="Entities/*.hbm.xml" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Repositories\" />
  </ItemGroup>
</Project>
