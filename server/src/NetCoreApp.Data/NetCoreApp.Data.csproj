<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>Beginor.NetCoreApp.Data</AssemblyName>
    <RootNamespace>Beginor.NetCoreApp.Data</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Beginor.AppFx.Core" Version="3.1.1" />
    <PackageReference Include="Beginor.AppFx.Repository.Hibernate" Version="3.1.1" />
    <PackageReference Include="Microsoft.Extensions.Identity.Stores" Version="3.1.0" />
    <PackageReference Include="Dapper" Version="2.0.30" />
    <PackageReference Include="NHibernate" Version="5.2.6" />
    <PackageReference Include="NHibernate.AspNetCore.Identity" Version="3.1.1" />
    <PackageReference Include="NHibernate.Extensions.NpgSql" Version="3.1.2" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Entities/*.hbm.xml" />
    <EmbeddedResource Include="Entities/*.hbm.xml" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Repositories\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NetCoreApp.Common\NetCoreApp.Common.csproj" />
    <ProjectReference Include="..\NetCoreApp.Models\NetCoreApp.Models.csproj" />
  </ItemGroup>
</Project>
