{
  "name": "net-core-app",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ngcc": "ngcc --create-ivy-entry-points -l debug",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web-style": "sass styles/web.scss styles/web.css --no-source-map --style=compressed",
    "build-web": "pnpm run build-web-style && ng build web --configuration production",
    "build-handset-style": "sass styles/handset.scss styles/handset.css --no-source-map --style=compressed",
    "build-handset": "pnpm run build-handset-style && ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "build-styles": "pnpm run build-web-style && pnpm run build-handset-style",
    "start": "pnpm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/net-core-app.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/net-core-app/issues"
  },
  "homepage": "https://github.com/beginor/net-core-app#readme",
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/router": "^16.2.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0",
    "@angular/localize": "^16.2.7",
    "bootstrap": "~5.2.3",
    "bootstrap-icons": "^1.11.1",
    "@popperjs/core": "^2.11.8",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "^16.2.6",
    "@angular/material": "^16.2.6",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "echarts": "^5.4.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.50.0",
    "ng-packagr": "^16.2.3",
    "typescript": "~5.1.3",
    "sass": "^1.68.0"
  }
}
