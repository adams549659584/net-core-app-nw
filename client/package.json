{
  "name": "net-core-app",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "build": "npm run build-web && npm run build-handset",
    "build-web": "ng build web --prod --base-href=/net-core-app/web/",
    "build-handset": "ng build handset --prod --base-href=/net-core-app/handset/",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage",
    "e2e": "ng e2e --webdriver-update false",
    "webdriver-update": "webdriver-manager update --proxy http://127.0.0.1:10080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/net-core-app.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/net-core-app/issues"
  },
  "homepage": "https://github.com/beginor/net-core-app#readme",
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "bootstrap": "^4.3.1",
    "@ng-bootstrap/ng-bootstrap": "^5.0.0-rc.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@angular/cdk": "~8.0.0",
    "@angular/material": "~8.0.0",
    "hammerjs": "^2.0.0",
    "material-design-icons": "^3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.4",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
