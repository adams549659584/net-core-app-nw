{
  "extends": "../tslint.json",
  "rules": {
    "one-line": [true, "check-open-brace", "check-whitespace"],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration"
    ],
    "member-access": true
  }
}
