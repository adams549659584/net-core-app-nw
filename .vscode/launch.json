{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Server",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet:build",
      "program": "${workspaceFolder}/server/src/NetCoreApp.Entry/bin/Debug/net6.0/NetCoreApp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/server/src/NetCoreApp.Entry",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": false,
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/server/src/NetCoreApp.Entry/Views"
      }
    },
    {
      "name": "Attache Process (.Net)",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
